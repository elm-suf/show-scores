{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pnpm exec nx run fe:build --configuration=production",
    "nixpacksConfigPath": "deploy/fe/nixpacks.toml"
  },
  "deploy": {
    "startCommand": "pnpm exec nx run fe:serve --configuration=production",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 5
  }
}
